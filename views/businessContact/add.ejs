<% include ../partials/header.ejs%>





<div class="container">
    <div class="raw">
        <div class="offset-md-3 col-md-6">
        <h1><%= title %></h1>

        <form class="form" method="POST">
            
            <div class="form-group">
                <label for="nameTextField"></label>
                <input type="text" class="form-control"
                id="nameTextField"
                placeholder="Enter Business Contact Name"
                name="Contact_name" required>
            </div>

            <div class="form-group">
                <label for="numberTextField"></label>
                <input type="text" class="form-control"
                id="numberTextField"
                placeholder="Enter Business Contact Number"
                name="Contact_number" required>
            </div>

            <div class="form-group">
                <label for="emailTextField"></label>
                <input type="text" class="form-control"
                id="emailTextField"
                placeholder="Enter Business Contact Email"
                name="Email" required>
            </div>
            <div style="display:inline-block">
            <button class="btb btn-primary bttn" type="submit">
                <i class="fas fa-edit " ></i> Submit
            </button>

            <a href="/Bcontact" class="btn btn-warning bttn" style="padding-top: 6px;"><i class="fas fa-undo "></i> Cancel</a>
        </div>    
        </form>
    </div>
    </div>
</div>






<% include ../partials/footer.ejs%>