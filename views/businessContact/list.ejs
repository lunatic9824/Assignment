<% include ../partials/header.ejs%>


  

<main class="container">
    <div class="row" >
        <div class="offset-md-1 col-md-9">
            
            <h1 style="text-align: center; color: cyan; text-decoration-line: underline"><%= title %></h1>
            <br><br>
            <% if(typeof displayName == 'string' && displayName) { %>
            <a href="/business-contact-list/add" class="btn btn-primary" style="margin-bottom: 10px;"role="button">
            <i class="fas fa-plus-circle" ></i>Add Business Contact</a>
            <% } %>
            <br>
            <table class="table table-bordered table-striped table-hover" >
               
                <tr>
                    <th class="text-center col-1" style="color: cyan">Contact Name</th>
                    <th class="text-center col-2" style="color: cyan">Contact Number</th>
                    <th class="text-center col-2"style="color: cyan">Email</th>
                    <% if(typeof displayName == 'string' && displayName) { %>
                    <th class="text-center col-1"style="color: cyan">Action</th>
                    <th class="text-center col-1"style="color: cyan">Action</th>
                    <% } %>
                </tr>

                
                <% for(let count =0;count<BusinessContactList.length;count++){ %>
                    <tr>
                        <td class="text-center" style="color: cyan"><%= BusinessContactList[count].Contact_name %></td>
                        <td class="text-center"style="color: cyan"><%= BusinessContactList[count].Contact_number %></td>
                        <td class="text-center"style="color: cyan"><%= BusinessContactList[count].Email %></td>
                        <% if(typeof displayName == 'string' && displayName) { %>
                        <td class="text-center col-1">
                            <a href="/business-contact-list/edit/<%=BusinessContactList[count]._id %>" class="btn btn-primary btn-sm">
                            <i class="fas fa-pencil-alt"></i> Edit</a>
                        </td>
                        <td class="text-center col-1">
                            <a href="/business-contact-list/delete/<%=BusinessContactList[count]._id%> " class="btn btn-danger btn-sm">
                            <i class="fas fa-trash-alt"></i> Delete</a>
                            </td>
                    
                        <% } %>
                    </tr>
               <% } %>

            </table>

        </div>
    </div>
</main>




<% include ../partials/footer.ejs%>